{
  "name": "@veerajshenoy/imageloader",
  "version": "0.0.7",
  "description": "Image Loader Component - Lazy loading and Progressive loading.",
  "keywords": ["stenciljs", "webcomponents", "lazy loading", "images"],
  "main": "dist/imageloader.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/stencil-starter-project-name/stencil-starter-project-name.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "devDependencies": {
    "@stencil/core": "^1.0.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/js-ninja/image-loader.git"
  },
  "author": "Veeraj Shenoy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/js-ninja/image-loader/issues"
  },
  "homepage": "https://github.com/js-ninja/image-loader",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
